#Always include our version of usr/bin if it's in the home dir first
PATH=~/usr/bin
#git on OS X
PATH=$PATH:/usr/local/git/bin
#Homebrew
PATH=$PATH:/opt/homebrew/bin:/opt/homebrew/sbin
#The usual
PATH=$PATH:/sbin:/usr/local/sbin:/usr/sbin:/usr/local/gnu-coreutils/bin:/usr/local/bin:/bin:/usr/bin
#Xcode Developer Tools
PATH=$PATH:/Developer/Tools
#OSX MySQL
PATH=$PATH:/usr/local/mysql/bin

# Perform the cleaning
pathclean PATH

